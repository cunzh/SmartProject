	<select id="selectByPage" parameterType="Integer" resultMap="userCartMap">
		select cart.id as id,product.id as productId,product.name
		,image.id as
		imageId,
		image.url,product.simple_description as
		simpleDescription,product.complex_description as
		complexDescription,
		product.shop_id as
		shopId,
		product.price,cart.user_id as
		userId,cart.num,cart.product_description
		as
		productDescription,cart.total_price as totalPrice
		from user left
		outer
		join cart on user.id =
		cart.user_id
		left outer join
